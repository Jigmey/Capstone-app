<h3>Welcome to the Friends List!</h3>
<p>Your Friends</p>
<%@user.friendships.each  do |friendship|%>   
    <a href="/generals/<%=friendship.friend_id%>"><p><img src="<%=friendship.friend.pic%>"><%=friendship.friend.name%></p></a>
    <%=form_tag("/friendships/deletefriend", method: :delete) do %>
        <%= hidden_field_tag(:friendship_id, friendship.id) %>
        <!--<%= friendship.id %>-->
        <%=submit_tag "Delete"%>
    <%end%>
<%end%>
